# Clerk - get keys from https://dashboard.clerk.com
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_REPLACE_WITH_YOUR_CLERK_PUBLISHABLE_KEY
CLERK_SECRET_KEY=sk_test_REPLACE_WITH_YOUR_CLERK_SECRET_KEY

# Supabase - get from your project Settings > API
NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
# Client key (use one; app accepts either name)
NEXT_PUBLIC_SUPABASE_ANON_KEY=REPLACE_WITH_YOUR_ANON_OR_PUBLISHABLE_KEY
# SUPABASE_PUBLISHABLE_KEY=...   # alternate name (same as above)
# Server key (use one; app accepts either name)
SUPABASE_SERVICE_ROLE_KEY=REPLACE_WITH_YOUR_SERVICE_ROLE_OR_SECRET_KEY
# SUPABASE_SECRET_KEY=...        # alternate name (same as above)

# Discovery service (for validate-discovery and feed push proxy)
DISCOVERY_SERVICE_URL=https://your-discovery-service.onrender.com

# Task Queue (for partner Tasks page)
TASK_QUEUE_SERVICE_URL=https://uso-task-queue.onrender.com

# Hub Negotiator (for partner Hub RFPs and platform RFPs)
HUB_NEGOTIATOR_SERVICE_URL=https://uso-hub-negotiator.onrender.com

# Hybrid Response (for Support / classify-and-route in portal)
HYBRID_RESPONSE_SERVICE_URL=https://uso-hybrid-response.onrender.com

# Orchestrator (for payment create proxy)
ORCHESTRATOR_SERVICE_URL=https://uso-orchestrator.onrender.com

# Payment service (for sponsorship create)
PAYMENT_SERVICE_URL=https://uso-payment.onrender.com

# Stripe (for payment page)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_REPLACE_WITH_YOUR_STRIPE_PUBLISHABLE_KEY
